{
  "name": "node-tor",
  "description": "Node.js Tor",
  "version": "1.0.3",
  "author": {
    "email": "simonegauli@gmail.com",
    "name": "Simone Gauli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pnlvfx/node-tor.git"
  },
  "keywords": [
    "tor",
    "tor-client",
    "proxy",
    "http",
    "socks",
    "socks5",
    "socket",
    "networking"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/tor.js",
  "module": "./dist/tor.js",
  "types": "./dist/tor.d.ts",
  "exports": {
    "types": "./dist/tor.d.ts",
    "import": "./dist/tor.js"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "unused": "node tools/unused.ts",
    "test": "node --import tsx --test tests/*.spec.ts",
    "lint": "eslint",
    "dev": "tsx --no-cache --watch tools/cli.ts",
    "build": "rimraf dist && eslint && tsc -p tsconfig.build.json",
    "prepare": "husky",
    "prepack": "yarn build",
    "prepublishOnly": "yarn test && eslint",
    "update": "yarn upgrade-interactive"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@goatjs/boarding": "^1.0.31",
    "@goatjs/core": "^1.0.31",
    "@goatjs/download": "^1.0.31",
    "@goatjs/node": "^1.0.31",
    "@goatjs/node-eslint": "^1.0.18",
    "@goatjs/pslist": "^1.0.31",
    "@goatjs/rimraf": "^1.0.31",
    "@goatjs/storage": "^1.0.31",
    "@goatjs/ts-unused-exports": "^1.0.31",
    "@goatjs/typescript-config": "^1.0.31",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "mime-types": "^3.0.1",
    "prettier": "^3.6.2",
    "tar": "^7.4.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "zod": "^4.0.9"
  },
  "peerDependencies": {
    "@goatjs/pslist": "*",
    "@goatjs/storage": "*",
    "zod": "*"
  },
  "bin": {
    "tor-install": "dist/manager/cli/cli.js"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
